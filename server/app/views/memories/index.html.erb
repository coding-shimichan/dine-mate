<p style="color: green"><%= notice %></p>

<% content_for :title, "Memories" %>

<h1>Memories</h1>

<div id="memories">
  <% @memories.each do |memory| %>
    <%= render memory %>
    <p>
      <%= link_to "Show this memory", user_memory_path(user_id: memory.user.id, id: memory.id) %>
    </p>
  <% end %>
</div>

<%= link_to "New memory", new_user_memory_path(user_id: @user.id) %>
