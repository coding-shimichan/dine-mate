<p style="color: green"><%= notice %></p>

<% content_for :title, "Messages" %>

<h1>Messages</h1>

<div id="messages">
  <% @messages.each do |message| %>
    <%= render message %>
  <% end %>
</div>

<div id="message-form">
    <%= render partial: "messages/form", locals: { chat: @chat, message: @message } %>
</div>